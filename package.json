    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "cz-git": "^1.8.0",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
