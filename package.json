      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*",
      "urix": "*",
      "resolve-url": "*",
      "source-map-url": "*",
      "sourcemap-codec": "*",
      "source-map-resolve": "*"
    }
  }
