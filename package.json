    "lint:eslint": "eslint --cache \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,ts,tsx,css,less,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz"
  },
