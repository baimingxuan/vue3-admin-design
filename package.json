    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.1.0",
    "axios": "^1.6.5",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "dom-to-image": "^2.6.0",
